{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "allowJs": true,
    "checkJs": false,
    "outDir": "./dist",
    "rootDir": ".",
    "strict": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": ["./src/*"]
    },
  },
  "exclude": ["node_modules", "dist"],
}